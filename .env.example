# 环境配置文件示例
# 复制此文件为 .env 并根据实际情况修改配置

# 基础配置
APP_NAME=LLM Proxy API（deno）
DEBUG=false
HOST=0.0.0.0
PORT=3333

# API Key配置
API_KEYS=["your-api-key-1", "your-api-key-2"]
REQUIRE_API_KEY=true

# 目标API配置
TARGET_API_URL=https://api.orionai.asia/chat
CONVERSATION_API_URL=https://api.orionai.asia/conversation
PROJECT_API_URL=https://api.orionai.asia/project
MESSAGE_API_URL=https://api.orionai.asia/conversation

# Token池配置 - 添加您的实际token
TOKEN_POOL=["your_token_1", "your_token_2", "your_token_3"]

# OrionAI 管理配置
# 用于删除对话的管理token (从浏览器 cookie 中获取)
ORION_TOKEN=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.example_token_here
# 是否在每次聊天前自动清理项目中的对话 (默认开启)
AUTO_CLEAN_CONVERSATIONS=true

# 模型配置 - 支持多种Agent类型
